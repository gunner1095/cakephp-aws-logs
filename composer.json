{
    "name": "gunner1095/cakephp-aws-logs",
    "description": "CakePHP log adapters for AWS",
    "homepage": "https://github.com/gunner1095/cakephp-aws-logs",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "AWS", "CloudWatch", "Kinesis", "Log"],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "~4.2.0",
        "aws/aws-sdk-php": "^3.175"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "~4.2.0",
        "phpunit/phpunit": "~8.5.0 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "AwsLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AwsLogger\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
